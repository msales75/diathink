{
    "name":"$D",
    "mode":"debug",
    "debugLevel":1,
    "eliminate":false,
    "minify":false,
    "preloadImages":false,
    "proxies":[
        {
            "baseUrl":"http://your.server.com",
            "proxyAlias":"myServer"
        },
        {
            "baseUrl":"http://your.server2.com",
            "proxyAlias":"myServer2"
        }
    ],
    "htmlHeader":[
        "<meta charset=\"utf-8\">",
        "<meta name=\"apple-mobile-web-app-capable\" content=\"yes\">",
        "<meta name=\"apple-mobile-web-app-status-bar-style\" content=\"default\">",
        "<link rel=\"apple-touch-icon\" href=\"theme/images/Icon-72.png\" />",
        "<link rel=\"apple-touch-icon\" href=\"theme/images/Icon.png\" />",
        "<link rel=\"apple-touch-icon\" href=\"theme/images/Icon@2x.png\" />",
        "<link rel=\"shortcut icon\" href=\"theme/images/favicon.ico\" type=\"image/x-icon\">"
    ],
    "application":{
        "version":"0.0.1",
        "debugMode":true,
        "useTransitions":true,
        "keyPrefix":"#m#",
        "keySuffix":"_",
        "timeStampCreated":"created_at",
        "timeStampUpdated":"updated_at",
        "m_id":"m_id"
    },
    "libraries":[
        {
            "name":"jquery-ui",
            "refs":[
                "diathink.jquery.js",
                "jquery.simulate.js",
                "jquery.stylesheet.js",
                "jquery.ui.core.js",
                "jquery.ui.widget.js",
                "jquery.ui.mouse.js",
                "jquery.ui.sortable.js",
                "jquery.ui.touch-punch.js",
                "jquery.ui.nestedSortable.js",
                "jquery.cursor.js",
                "textinput.js"
            ]
        },
        {
            "name":"backbone",
            "refs":[
                "json2.js",
                "underscore.js",
                "backbone.js",
                "backbone.localStorage.js",
                "backbone-relational.js",
                    "backbone-integration.js"
            ]
        }
    ]
}
